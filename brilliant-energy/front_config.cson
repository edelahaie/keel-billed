          label:
            text: "Haute disponibilité"
            position: "top"
        }
      ]
      value: "Disponibilité"
      groups: "Centrale"
      date: "mois"
      chartType: "linechart"
      data:
        date:
          selector: "mois"
          format: "%m"
        query:
          domain: "data_centrales_evolution"
          Centrale:
            $in: [
              "<%= report.entityName %>"
              "Moyenne <%= report.entityTechno %>"
            ]
        precision:
          "Disponibilité": ".0%"
        postprocess: [
          {
            rename:
              columns:
                dispo_dyn_fake:
                  fr: "Disponibilité"
                  en: "Disponibilité"
          }
        ]
      legend: true
  }
