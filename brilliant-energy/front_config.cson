      units:
        value: " M€"
      chartType: "waterfallchart"
      data:
        query:
          domain: "data_waterfall-budget"
        precision:
          value: ".1f"
  }
  {
    highlightedKpis: [
      {
        comment: "Moyenne de la disponibilité réalisée sur l'année"
        name: "Disponibilité cumulée"
        sentiment:
          dispo:
            domain: [
              0.95
            ]
            range: [
              "negative"
              "positive"
            ]
        precision:
          dispo: ".1%"
        value: "dispo"
        data:
          dispo:
            query:
              report: "<%= report.entityName %>"
              domain: "data_reports"
            precision:
              "Disponibilité Réalisée": ".1%"
            field: "Disponibilité Réalisée"
      }
    ]
    level: 3
    title: "Evolution des indicateurs par Centrale"
    restrictVisibility:
      reportEntry: "entityType"
      allowedReports: [
        "CENTRALE"
      ]
    parent_id: 10
    filters:
      "bottom-right":
        on: "variable"
        type: "buttons"
    recommendation:
      advert: "INFO"
      content: "L'indicateur peut être modifié à l'aide des boutons en bas de l'écran"
    commentary: "Comment ont évolué mes indicateurs cette année ?"
    id: 1003
    chartOptions:
      value: "value"
      events: [
        {
          date: "8"
          name: "Pic de production d'août"
        }
        {
          date: "11"
          name: "Pic de production de novembre"
        }
      ]
      units:
        value: [
          {
            where:
              variable: "Production (MWH)"
            unit: " MHW"
          }
          {
            where:
              variable: "CA (k€)"
            unit: " k€"
          }
          {
            where:
              variable: "Prix MWh (€)"
            unit: " €"
          }
        ]
      date: "mois"
      chartType: "linechart"
      data:
        date:
          selector: "mois"
          format: "%m"
        query:
          domain: "data_centrales_evolution"
          Centrale: "<%= report.entityName %>"
        precision:
          value: [
            {
              where:
                variable: "Prix MWh (€)"
              precision: ".1f"
            }
            {
              where:
                variable: "Facteur de charge"
              precision: ".1%"
            }
          ]
        postprocess: [
          {
            melt:
              id: [
                "mois"
              ]
              value: [
                "Production (MWH)"
                "CA (k€)"
                "Facteur de charge"
                "Prix MWh (€)"
              ]
          }
        ]
      legend: true
  }
  {
    level: 3
    title: "Disponibilité au long de l'année"
    restrictVisibility:
      reportEntry: "entityType"
      allowedReports: [
        "CENTRALE"
      ]
    parent_id: 10
    recommendation:
      advert: "ATTENTION !"
      content: "La Disponibilité ici est une simple remise en échelle de du Facteur de charge (données non réalistes)"
    commentary: "Quand la <b><em><span class=\"legend-text-line\"> Disponibilité</span></em></b> dépasse les seuils définis ?"
    id: 1004
    chartOptions:
      sentimentZones: [
        {
          domain: [
            0
            0.95
          ]
          sentiment: "negative"
          label:
            text: "En dessous du seuil minimal de disponibilité"
            position: "top"
        }
        {
          domain: [
            0.99
            1
          ]
