settings:
  comparison:
    description: "Sur tous les écrans de comparaison, comparer avec :"
    values: [
      "Par défaut"
      "Entity A"
      "Entity B"
      "Entity C"
    ]
    default: "Par défaut"

glossary:
  title: 'Glossaire'
  terms: [
    term: "Churn rate"
    meaning: "the percentage of subscribers to a service who discontinue their subscriptions to that service within a given time period"
  ,
    term: "Activation"
    meaning: "Number of clients who went from inactive to active within 12 months"
  ,
    term: "Deactivation"
    meaning: "Number of clients who went from active to inactive within 12 months"
  ]

slidePdfEnabled: false
dashboardPdfEnabled: false
sharingOptions: true
comments: true
embedCharts: true
offline: 'all'

noDataError:
