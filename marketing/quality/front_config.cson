        level: "drill_level"
        levelsOrder: ['main','sub']
      quadrants:
        xDecile:5
        yDecile: 5
        labels: ['Perte de vitesse','Trajectoire d\'excellence','Décrochage','Zone d\'ascension']
        sentiments: ['warning','positive','negative','warning']
      fixedScale: false
      legend:
        cy: "situation"
        cx: "evolution"
        r: "clients"
