,
  name: "Search"
  kpis: [
    name: "Score Card"
    type: 'score-card'
    slide: 4002
    chartOptions:
      value: "value"
      variation: "evol"
      label: "label"
      target: "target"
      limit: 6
      sentiment:
        evol:
          domain:[0]
          range:['negative', 'positive']
      reverse: false
      units:
        evol: " %"
        value: [
            unit: ""
            where:
              label: "IMPR"
          ,
            unit: ""
            where:
              label: "CLICS"
          ,
            unit: " %"
            where:
              label: "CTR"
          ,
            unit: ""
            where:
              label: "CONV"
          ,
            unit: "  pts"
            where:
              label: "QUALITY SCORE"
          ,
            unit: "%"
            where:
              label: "Maturists"
          ]
      data:
        query:
          domain: "scorecard"
          type: "web"

