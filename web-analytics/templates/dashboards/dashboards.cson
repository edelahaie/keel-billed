entityName: "{{ zone }}"
id: "{{ index }}"
report: "{{ report_id }}"
default: "{{ default }}"


categories: [
  name: "Web"
  kpis: [
    name: "Visits"
    type: 'value-dynamic'
    value: "value"
    comment: "<%=\' \' + evolution + \' vs Feb 2016\'%>"
    unit: " k visits"
    slide: 10001
    badge: 'evolution'
    sentiment:
      evolution:
        domain: [0]
        range: ['negative','positive']
    precision:
      value: ',.0f'
      evolution: "+,.0%"
    data:
      value:
        query:[
          $match:
            domain: "average"
            zone: "Web global"

        ]
        field: "value"
      comparison:
        query:[
          $match:
            domain: "average"
            zone: "Web global"
        ]
        field: "budget"
    compute:
      evolution: '(value - comparison)/comparison'
    # source: "2016, source Smart Toucan"
    id: 0
  ,
    name: "New Visitors"
    type: 'value-dynamic'
    value: "value"
    comment: "<%=\' \' + evolution + \' vs 2015\'%>"
    unit: " k visitors"
    slide: 10001
    badge: 'evolution'
    sentiment:
      evolution:
        domain: [0]
        range: ['negative', 'neutral', 'positive']
    precision:
      value: ',.0f'
      evolution: "+,.0%"
    data:
      value:
        query:[
          $match:
            domain: "average"
            zone: "Visitors global"

        ]
        field: "value"
      comparison:
        query:[
          $match:
            domain: "average"
            zone: "Visitors global"

        ]
        field: "budget"
    compute:
      evolution: '(value - comparison)/comparison'
    # source: "2016, source Smart Toucan"
    id: 1
  ,
    name: "Bounce Rate"

    type: 'value-dynamic'
    value: "value"
    comment: "<%=\' \' + evolution + \' vs 2015\'%>"
    unit: " %"
    slide: 10002
    badge: 'evolution'
    sentiment:
      evolution:
        domain: [0]
        range: ['negative','positive']
    precision:
      value: ',.0f'
      evolution: "+,.0%"
    data:
      value:
        query:[
          $match:
            domain: "average"
            zone: "Bounce global"
        ]
        field: "value"
      comparison:
        query:[
          $match:
            domain: "average"
            zone: "Bounce global"

        ]
        field: "budget"
    compute:
      evolution: '(value - comparison)/comparison'
    # source: "2016, source Smart Toucan"
    id: 2
   ]
,
  name: "Mobile"
