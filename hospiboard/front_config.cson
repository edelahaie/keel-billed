  {
    level: 3
    parent_id: 20
    id: 2001
    title: "Part de marché"
    commentary: "Part de marché de l'établissement au sein de la <%= requestersManager.zones %>"
    filters:
      'bottom-right':
        on: 'metric'
        type: 'buttons'
        default: "Médecine"
        datasets: [0,1]
    charts:[
      {
        data:
          query:
            domain: "part_de_marche"
            etablissements: "<%= report.entityName %>"
            # metric: '<%= requestersManager.metric %>'
            zones: '<%= requestersManager.zones %>'
          precision:
            value: '.1f'
          date:
            format: "%Y"
        chartType: "linechart"
        value: "value"
        groups: 'etablissements'
        date: 'date'
        units:
          value: "%"
        crossfilter:
          on: "date"
          datasets: [1]
        style: "secondary"
        id: 0
        size: 0.25
      }
      {
        data:
          query:
            domain: "leaderboard"
            zones: '<%= requestersManager.zones %>'
          precision:
            value: '.1f'
            variation: '.1f'
        chartType: "horizontal-barchart"
        label: "etablissements"
        value: "value"
        sort: 'desc'
        rank: true
        variation: "variation"
        variationLegend: "vs.N-1"
        # legend: 'Part de marché'
        # filters: 'upper-middle':
        #   on: 'metric'
        #   type: 'buttons'
        units:
          value: "%"
          variation: ' pts'
        id: 1
      }
    ]
