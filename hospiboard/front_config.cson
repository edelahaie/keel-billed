  {
    level: 3
    parent_id: 10
    id: 1004
    title: "Activité pour les interventions"
    source: "Source PMSI"
    commentary: "Activité pour les interventions les plus fréquentes au niveau national"
    # filters:
    #   'bottom-right':
    #     on: 'Année'
    #     type: 'slider'
    #     defaultSelected: "2015"
    # chartOptions:
    #   chartType: "stackedBarchart"
    #   data:
    #     precision: 0
