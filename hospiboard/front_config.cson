    #   order: "value"
    #   sort: "desc"
    #   legend:
    #     type: 'identity-card'
    #     filter: 'bottom-right'
    chartOptions:
      data:
        query:
          domain: 'Carte_identite_Act'
          etablissements: "<%= report.entityName %>"
          date: '<%= requestersManager.date.date %>'
          metric: "Intervention"

      chartType: "horizontalBarchart"
      value: "value"
      label: "Label"
      variation: "Evolution"
      variationLegend: "Écart<br>année précédente"
      units:
        value: " "
        Evolution: " "

      sort: "desc"
      sparklines:
        data:
          query:
            domain: 'Carte_identite_Act'
            etablissements: "<%= report.entityName %>"
            metric: "Intervention"
        joins: ["Label"]
        legend: "Évolution<br>sur 4 ans"
        value: "value"
        date: "date"
        Label : "Dates"
        unit: "actes"
        orderDates: true
      showAverage: true
  }
