  {
    level: 3
    parent_id: 20
    id: 2003
    title: "Occupation des lits"
    commentary: "Comparaison au sein de la <%= requestersManager.comparaison %><br>Selectionnez dans l'onglet de configuation l'élément de comparaison<br>région, catégorie, typologie"
    filters:
      'bottom-right':
        on: 'date'
        type: 'slider'
        # default: "Médecine"
    chartOptions:
      comparison: '2ème décile'
      commonScale: true
      target: '8ème décile'
      # 'packs': 'group'
      value: 'value'
      label: 'label'
      meta: [ '' ]
      units:
        value: '%'
        '8ème décile': '%'
        '2ème décile': '%'
      chartType: 'bulletChart'
      data:
        query:
          domain: 'occupation_lits'
          etablissements: "<%= report.entityName %>"
          comparaison: '<%= requestersManager.comparaison %>'
          # date: '<%= requestersManager.date.date %>'
  }
