  requesters:[
      id:"zones"
      description: "Zones"
      type:"dropdown"
      default:"zone d'attractivité"
      query:
        domain:"part_de_marche"
      fields:["zones"]
      slides:[2001]
    ,
      id:"comparaison"
      description: "Comparer avec"
      type:"dropdown"
      default:"région"
      query:
        domain:"occupation_lits"
      fields:["comparaison"]
      slides:[2003,2004,4001,4003]
  ]


