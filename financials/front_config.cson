  {
    level: 3
    title: "Imposition"
    commentary: "Taxes in 2012"
    filters:
      "bottom-right":
        on: "variable"
        type: "buttons"
    parent_id: 30
    chartOptions:
      data:
        query:
          domain: "feminisation"
          "Montant de l'impot":
            $ne: 0
        melt:
          id: [
            "company"
            "sector"
          ]
          value: [
            "Taux d'imposition"
            "Montant de l'impot"
          ]
      chartType: "horizontalBarchart"
      label: "company"
      value: "value"
      average:
        data:
          query: [
            {
              $match:
                domain: "feminisation"
                "Montant de l'impot":
                  $ne: 0
            }
            {
              $group:
                _id: [
                  "index"
                ]
                "Taux d'imposition":
                  $avg: "$Taux d'imposition"
                "Montant de l'impot":
                  $avg: "$Montant de l'impot"
            }
            {
              $project:
                index: "$_id.index"
                "Taux d'imposition": 1
                "Montant de l'impot": 1
            }
          ]
          melt:
            id: [
              "index"
            ]
            value: [
              "Taux d'imposition"
              "Montant de l'impot"
            ]
        joins: [
          "variable"
        ]
        value: "value"
      averageText: "moyenne CAC40"
      filters:
        "upper-right":
          on: "sector"
          type: "dropdown"
          reset: "- Tous secteurs"
          withSize: true
      units:
        value: [
          {
            unit: "%"
          }
          {
            unit: "M€"
            where:
              variable: "Montant de l'impot"
          }
        ]
      legend:
        type: "identity-card"
        filter: "bottom-right"
      sort: "desc"
      meta: []
    source: "Source : PwC"
    id: 4
  }
  {
