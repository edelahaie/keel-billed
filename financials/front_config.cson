        multiple_queries: true
      chartType: "horizontalBarchart"
      label: "label"
      value: "value"
      filters:
        "upper-right":
          on: "company"
          type: "dropdown"
      average:
        data:
          query: [
            [
              {
                $match:
                  domain: "board"
              }
              {
                $group:
                  _id: [
                    "$index"
                  ]
                  value:
                    $avg: "$age"
              }
              {
                $project:
                  company:
                    $literal: "- All companies"
                  value: 1
              }
            ]
            [
              {
                $match:
                  domain: "board"
              }
              {
                $group:
                  _id: "$company"
                  value:
                    $avg: "$age"
              }
              {
                $project:
                  company: "$_id"
                  value: 1
              }
            ]
          ]
          multiple_queries: true
        joins: [
          "company"
        ]
        value: "value"
      averageText: "moyenne CAC40"
      units:
        value: " "
      legend: true
      sort: "desc"
      meta: [
        "since"
        "position"
      ]
