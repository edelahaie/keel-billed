  {
    level: 3
    title: "Top Entreprises"
    parent_id: 10
    filters:
      "bottom-right":
        on: "metric"
        type: "buttons"
        default: "Chiffre d'affaires"
        order:
          custom: [
            "Chiffre d'affaires"
            "Résultat net"
            "Capitaux propres"
            "Marge opérationnelle"
            "Rentabilité Financière"
          ]
    chartOptions:
      data:
        query: [
          [
            {
              $match:
                domain: "data_bourso"
                metric:
                  $in: [
                    "Chiffre d'affaires"
                    "Résultat net"
                    "Capitaux propres"
                  ]
                year: "2016"
            }
            {
              $project:
                metric: 1
                value:
                  $divide: [
                    "$value"
                    1000000
                  ]
                company: 1
                supersector: 1
            }
          ]
          [
            {
              $match:
                domain: "data_bourso"
                metric:
                  $in: [
                    "Marge opérationnelle"
                    "Rentabilité Financière"
                  ]
                year: "2016"
            }
            {
              $project:
                metric: 1
                value: 1
                company: 1
                supersector: 1
            }
          ]
        ]
        multiple_queries: true
        precision:
          value: ",.1f"
      chartType: "horizontalBarchart"
      label: "company"
      units:
        value: [
          {
            unit: "Md€"
          }
          {
            unit: " "
            where:
              metric: "Marge opérationnelle"
          }
          {
            unit: " "
            where:
              metric: "Rentabilité Financière"
          }
        ]
      value: "value"
      filters:
        "upper-right":
          on: "supersector"
          type: "dropdown"
          reset: "- Tous secteurs"
          withSize: true
      sparklines:
        data:
          query:
            domain: "data_bourso"
            metric:
              $in: [
                "Chiffre d'affaires"
                "Résultat net"
                "Capitaux propres"
                "Marge opérationnelle"
                "Rentabilité Financière"
              ]
            year:
              $ne: "2017"
        joins: [
          "metric"
          "company"
        ]
        legend: "Évolution"
        value: "value"
        date: "year"
        unit: " "
        orderDates: true
      legend:
        type: "identity-card"
        filter: "bottom-right"
      sort: "desc"
      meta: []
    source: "Source : Rapports financiers"
    id: 10002
  }
