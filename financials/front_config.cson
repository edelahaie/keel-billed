settings: []
glossary:
  title: "Glossaire"
  terms: [
    {
      term: "CAC 40"
      meaning: "Le CAC 40 (cotation assistée en continu) est le principal indice boursier de la Bourse de Paris. Son code ISIN est FR0003500008 et son code mnémonique est PX1."
    }
    {
      term: "Chiffre d'affaires"
      meaning: "La somme des ventes de biens ou de services d'une entreprise sur un exercice comptable. Il peut inclure ou non des achats pour la revente et des sous-traitance selon le mode de comptabilisation retenu."
    }
    {
      term: "Graph"
      meaning: "A slang term for a sketching of what someone plans on tagging. graffiti. <br><em>NOT A CHART!</em>"
    }
  ]
