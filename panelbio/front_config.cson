      chartType: 'horizontal-barchart'
      units:
        value: " €"
      data:
        query:[
          $match:
            domain: "panel_periode_365"
            "<%= report.ENTITY_COLUMN_ID %>": "<%= report.ENTITY_ID %>"
        ,
          $group:
            _id:
              reseau: "$reseau_label"
            value: $sum: "$achat_prix_total_ttc"
        ,
          $project:
            reseau: "$_id.reseau"
            value_euros: $divide: ["$value", 100]
        ]
      value: "value_euros"
      label: "reseau"
      sort: "desc"
  }
