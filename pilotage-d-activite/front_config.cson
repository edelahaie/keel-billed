    parent_id: 10
    id: 101
  }
  {
    level: 2
    title: "Entreprises & Territoires"
    parent_id: 10
    id: 102
  }
  {
    level: 3
    title: "Evolution mensuelle des indicateurs"
    parent_id: 101
    id: 10101
    source: "Source Société Atac, les données du mois, en non comparable"
    # commentary: '''
    #   La tendance sur 12 mois glissants ...

    #   '''
   # recommendation:
   #    advert: "Detail"
   #    content: "La formule de vente en images <br><br> <img src='images/formule de vente.png'></img>"
    chartOptions:
      chartType: "linechart"
      data:
        precision:
          Value: ".2f"
        date:
          selector: "Date"
          format: "%m/%Y"
        query:
          domain: "progress_metrics"

      date: "Date"
      groups: "Label"
      legend: true
      units:
        "Value": " %"
        date: " "
      value: "Value"
  }
  {

    level: 3
    title: "Progressions"
    parent_id: 102
    id: 10202
    source: "Source Société Atac, données non comparables, A fin août 2015"
    # commentary:
    #   "Articles": "A fin août 2015 ..."
    #   "Articles par client": "A fin août 2015 ..."
    #   "Chiffre d'affaires": "A fin août 2015 ..."
    #   "Clients": "A fin août 2015 ..."
    #   "Panier moyen": " A fin août 2015 ..."
    #   "Prix de vente moyen": "A fin août 2015 ..."
    chartOptions:
      chartType: "leaderboardCenteredAverage"
      data:
        order:
          by: "Label"
          custom: [
            "Nord-Est",
            "Ouest",
            "Sud",
            "Alsace",
            "Lorraine",
            "Nord Picardie",
            "Normandie",
            "Aquitaine",
            "Auvergne",
            "Méditerranée",
            "Rhône Alpes",
            "Grand Ouest",
            "IDF Sud",
            "Paris",
            "Paris Nord"
          ]
        precision:
          Value: ".0f"
        query:
          domain: "growths"
      packs: "Pack"
      packNames: false
      value: "Value"
      label: "Label"
      units:
        Value: "%"
      tutorial:
        id: "bottom_right"
        content: "Cliquez sur les boutons en bas à droite pour faire varier les axes d'analyse"

      average: " "
      averageText: '0'
      legend: false

    filters:
      'bottom-right':
        on: "Breakdown"
        type: "buttons"
  }
  {
    level: 3
    title: "Podiums"
    parent_id: 102
    id: 10204
    source: "Source Société Atac, données non comparables, août 2015"
    # commentary: {
    #   "Articles": "A fin août 2015 ..."
    #   "Articles par client": "A fin août 2015 ..."
    #   "Chiffre d'affaires": "A fin août 2015 ..."
    #   "Clients": "A fin août 2015 ..."
    #   "Panier moyen": " A fin août 2015 ..."
    #   "Prix de vente moyen": "A fin août 2015 ..."
    # }

    chartOptions:
      chartType: "horizontalBarchart"
      packs: "Pack"
      packNames: true
      data:
        query:
          domain: "ranking"
        precision:
          Value:',.1f'
          Growth:'.2f'
      label: "Label"
      value: "Value"
      sort: "desc"
      variation: "Growth"
      variationLegend: "Evolution<br>sur n-1"
      sparklines:
        prefix: "sparkline"
        legend: "Evolution<br>sur 12 mois"
        unit: " "
      units:
        Value: [
          unit: " k€"
          where:
            Breakdown: "Chiffre d'affaires"
        ,
          unit: " k"
          where:
            Breakdown: "Clients"
        ,
          unit: " "
          where:
            Breakdown: "Articles par client"
        ,
          unit: " €"
          where:
            Breakdown: "Panier moyen"
        ,
          unit: " €"
          where:
            Breakdown: "Prix de vente moyen"

        ,
          unit: " M"
          where:
            Breakdown: "Articles"
        ]
        Growth: " %"
    filters:
      'bottom-right':
        on: "Breakdown"
        type: "buttons"
  }
  {
    level: 3
    title: "Ecart sur objectif"
    parent_id: 102
    id: 10205
    source: "Source Société Atac, source société atac, CA du mois / objectif, non comparable"
    # commentary: "A fin août 2015 ..."
    chartOptions:
      chartType: "bulletChart"
      label: "Label"
      packs: "Pack"
      packNames: true
      data:
        precision:
          "Août 2015": ".0f"
          "Objectifs Août 2015": ".0f"
          "progression": ".0f"
        query:
          domain: "target_metrics"
        order:
          by: "Label"
          custom: [
            "France",
            "Nord-Est",
            "Ouest",
            "Sud",
            "Alsace",
            "Lorraine",
            "Nord Picardie",
            "Normandie",
            "Aquitaine",
            "Auvergne",
            "Méditerranée",
            "Rhône Alpes",
            "Grand Ouest",
            "IDF Sud",
            "Paris",
            "Paris Nord"
          ]
      value: "Août 2015"
      target: "Objectifs Août 2015"
      meta: ["progression"]
      alignTargets: true
      colorTargets: true
      units:
        "progression": "% par rapport à l'objectif"
        "Août 2015": " K€"
        "Objectifs Août 2015": " K€"

  }
